  },
  "homepage": "https://github.com/ramoh/hobnob#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "eslint": "^6.1.0",
    "husky": "^3.0.4",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4"
