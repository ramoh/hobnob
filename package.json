{
  "name": "hobnob",
  "version": "0.1.0",
  "description": "A sample app on node ",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && npx babel src -d dist",
    "serve": "yarn run build && node dist/index.js",
